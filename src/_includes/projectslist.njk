<div class="{{ class }}">
  {% if class == "projectNavMusic" %}
    <iframe width="100%" height="300" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url={{ project.url }}&hide_related=true&show_comments=false&show_user=true&show_reposts=false&show_teaser=false&visual=true"></iframe>
  {% elif project.img %}
    <a href="{{ project.url | url }}"><img src="{{ project.img }}"></a>
  {% endif %}

  {% if class == "latestItem" %}
    <div>
      <h3><a href="{{ project.url | url }}">{{ project.title }}</a></h3>
      <p class="caption"><time datetime="{{ project.date | htmlDateString }}">{{ project.date | readableDate }}</time></p>
      <!--Postmortem Link Here-->
    </div>
  {% else %}
    <h2><a href="{{ project.url | url }}">{{ project.title }}</a></h2>
    <p class="caption"><time datetime="{{ project.date | htmlDateString }}">{{ project.date | readableDate }}</time></p>
    <!--Postmortem Link Here-->
  {% endif %}
</div>