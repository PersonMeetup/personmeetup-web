{
	"version": "https://jsonfeed.org/version/1.1",
	"title": "{{ site.title }}",
	"language": "{{ site.language }}",
	"home_page_url": "{{ site.url }}",
	"feed_url": "{{ site.url }}/feeds/{% block self %}blog.json{% endblock %}",
	"description": "{% block subtitle %}{{ site.description }}{% endblock %}",
	"author": {
		"name": "{{ site.author.name }}",
		"url": "{{ site.author.url }}"
	},
	"items": [
		{% block entries %}
		{% endblock %}
	]
}
