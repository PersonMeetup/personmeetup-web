---
pagination:
  data: types
  size: 1
  alias: type
  addAllPagesToCollections: true
types:
  - Games
  - Music
  - Video
  - Miscellaneous
layout: layouts/browse-portfolio.njk
eleventyComputed:
  title: "{{ type }}"
  eleventyNavigation:
    key: "{{ type }}"
    parent: Portfolio
    order: 1
permalink: /portfolio/{{ type | slugify }}/
---
{% set filter = type %}
{% set projectslist = collections.portfolio %}

{% set class = "project-item" %}
{% for project in projectslist | reverse %}
	{% if filter == project.data.type %}
		{% include "projectslist.njk" %}
	{% endif %}
{% endfor %}