---
pagination:
  data: types
  size: 1
  alias: type
  addAllPagesToCollections: true
types:
  - Games
  - Music
  - Video
  - Web Design
layout: layouts/browse-portfolio.njk
eleventyComputed:
  title: "{{ type }}"
permalink: /portfolio/{{ type | slugify }}/
---
{% set filter = type %}
{% set projectslist = projects %}

{% if not (type == "Music") %}
  {% set class = "projectNavGeneral" %}
  {% for project in projectslist | reverse %}
    {% if filter == project.type %}
      {% include "projectslist.njk" %}
    {% endif %}
  {% endfor %}
{% else %}
  {% set class = "projectNavMusic" %}
  {% for project in projectslist | reverse %}
    {% if filter == project.type %}
      {% include "projectslist.njk" %}
    {% endif %}
  {% endfor %}
{% endif %}