{
	"name": "personmeetup-web",
	"version": "0.7.0",
	"description": "Personal website for Leslie 'Person Meetup' Swan",
	"main": "index.js",
	"scripts": {
		"watch:sass": "sass --watch src/_includes:src/_includes src/sass:dist/css",
		"build:sass": "sass src/_includes:src/_includes src/sass:dist/css",
		"watch:eleventy": "eleventy --serve --quiet",
		"build:eleventy": "eleventy",
		"start": "npm-run-all build:sass --parallel watch:sass watch:eleventy",
		"build": "npm-run-all build:sass build"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0",
		"@11ty/eleventy-fetch": "^3.0.0",
		"@11ty/eleventy-img": "^2.0.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"@11tyrocks/eleventy-plugin-sass-lightningcss": "^1.0.2",
		"@arothuis/markdown-it-biblatex": "^1.0.1",
		"cheerio": "^1.0.0-rc.12",
		"css-select": "^5.1.0",
		"dotenv": "^16.0.3",
		"eleventy-sass": "^2.2.1",
		"highlight.js": "^11.7.0",
		"luxon": "^2.5.2",
		"markdown-it": "^12.3.2",
		"markdown-it-anchor": "^8.6.7",
